  "scripts": {
    "start": "yarn run build && node dist/server.js",
    "dev": "concurrently \"cross-env NODE_ENV=development webpack --watch --mode=development\" \"cd ../gameserver && cross-env NODE_ENV=development webpack --watch --mode=development\"",
    "dev-reinit-db": "cross-env NODE_ENV=development FORCE_DB_REFRESH=true webpack --watch --mode=development",
    "build": "cross-env NODE_ENV=production webpack --mode=production",
    "test": "echo \"TODO: no test specified\" && exit 0",
    "localbuild": "yarn build && cross-env LOCAL_BUILD=true yarn start",
    "lint": "eslint --fix --ext \"**/*.js\" \"**/*.ts\"",
    "validate": "yarn run lint && yarn run build && yarn run test",
    "precommit": "no-master-commits -b master,dev",
    "upload-avatar": "node ./scripts/upload-avatar.js",
    "serve-local-files": "http-server ./upload --ssl --cert ../../certs/cert.pem --key ../../certs/key.pem --port 8642 --cors=*"
  },
